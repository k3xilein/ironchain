{
  "name": "iron-chain",
  "version": "1.0.0",
  "description": "Professional Solana Trading Bot",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:live": "RUN_MODE=MAINNET_LIVE npm start",
    "start:paper": "RUN_MODE=PAPER_LIVE npm start",
    "dev": "ts-node src/index.ts",
    "dev:paper": "RUN_MODE=PAPER_LIVE ts-node src/index.ts",
  "check-price": "ts-node src/scripts/check-price.ts",
    "force-breakout-test": "ts-node src/scripts/force-breakout-test.ts",
    "report": "ts-node src/scripts/report.ts",
    "status": "ts-node src/scripts/status.ts",
    "health": "ts-node src/scripts/health-check.ts",
    "export": "ts-node src/scripts/export.ts",
  "check:preload": "ts-node src/scripts/check-preload.ts",
    "kill": "touch STOP_AND_FLATTEN",
    "clean": "rm -rf dist"
  },
  "keywords": ["solana", "trading", "bot"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "technicalindicators": "^3.1.0",
    "dotenv": "^16.3.1",
    "better-sqlite3": "^9.2.2",
    "axios": "^1.6.2",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/better-sqlite3": "^7.6.8",
    "@types/bs58": "^4.0.4",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}